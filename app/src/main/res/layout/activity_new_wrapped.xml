            android:layout_gravity="center"
            android:layout_weight="1"
            android:paddingTop="15dp"
            android:text="2"
            android:textColor="@color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/album3"
            android:layout_width="wrap_content"
