            android:layout_weight="1"
            android:paddingTop="15dp"
            android:text="2"
            android:textColor="@color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/artist3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
